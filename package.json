{
  "name": "react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint:fix": "eslint --fix \"src/**/*.{ts,tsx,js,jsx,cjs,mjs}\"",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx,cjs,mjs}\"",
    "prettier": "prettier  \"**/*.+(json|yml|css|md|mdx|ts|tsx)\" --check",
    "prettier:fix": "prettier --write \"**/*.+(json|yml|css|md|mdx|ts|tsx)\"",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "test:unit": "vitest run --coverage"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.62.0",
    "@types/node": "^22.10.1",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.0.1"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.61.6",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.1.6",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.16.0",
    "vite": "^5.1.4",
    "vitest": "^2.1.6"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
